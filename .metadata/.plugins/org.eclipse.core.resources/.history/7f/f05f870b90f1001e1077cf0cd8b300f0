package com.java.core.day1session1;

public class BookImplementation {
	public BookImplementation() {
		Book b=new Book();
	}
	public void createBooks() {
		b.setBook_price(350.00);
		b.setBook_title("Java Programming");	
	}
	
	public Book showBooks() {
		Book b2=new Book();
		b2.getBook_price();
		b2.getBook_title();
		return b2;
	}
}
